{
  "name": "dispoflare",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "npm run wrangler",
    "start": "npm-run-all build --parallel \"dev:*\"",
    "typecheck": "tsc",
    "wrangler": "wrangler pages dev ./public",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@picocss/pico": "^1.5.7",
    "@remix-run/cloudflare": "^1.13.0",
    "@remix-run/cloudflare-pages": "^1.13.0",
    "@remix-run/react": "^1.13.0",
    "@sentry/remix": "^7.35.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "toucan-js": "^3.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@remix-run/dev": "^1.13.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "wrangler": "^3.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
